https://leetcode.com/problems/contains-duplicate/

static int speedup=[](){
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    return 0;
}();


class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
      if(nums.size()<2) {
          return false;
      }  
      sort(nums.begin(),nums.end());
      for(int i=0;i<nums.size()-1;i++) {
          if(nums[i]==nums[i+1]) {
              return true;
          }
      }
        return false;
    }
};
